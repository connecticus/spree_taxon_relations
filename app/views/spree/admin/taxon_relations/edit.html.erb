<% content_for :page_actions do %>
  <li><%= button_link_to Spree.t(:back_to_taxon_relations_list), '/admin/taxon_relations', :icon => 'icon-arrow-left' %></li>
<% end %>
<%= form_for @taxon_relation, url:admin_taxon_relation_path(@taxon_relation) do |f| %>

  <fieldset class="no-border-top">
    <%= render :partial => 'form', :locals => { :f => f } %>
    <div class="form-buttons filter-actions actions" data-hook="buttons">
      <%= button Spree.t('actions.update'), 'icon-ok' %>
    </div>
  </fieldset>
<% end %>
